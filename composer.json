{
    "name": "phlylabs/create-thumbnail",
    "description": "A simple class to create thumbnails from larger images",
    "license": "MIT",
    "keywords": ["PHP","Thumbnail"],
    "homepage": "http://phlylabs.com/",
    "authors": [
        {
            "name": "Matthias Sommerfeld",
            "email": "mso@phlylabs.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "suggest": {
        "ext-imagick": "It is recommended to install the ImageMagick package and PHP extension",
        "ext-gd": "Optionally install GD for the actual conversion process"
    }
    "repositories": {
        "type": "vcs",
        "url": "https://github.com/phlylabs/create-thumbnail.git"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Phlylabs\\CreateThumbnail\\": "src"
        }
    }
}
